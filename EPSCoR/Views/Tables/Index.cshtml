@model IEnumerable<EPSCoR.Models.Table>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

@if(ViewBag.UploadResult != null) {
    <div id="UploadResult">
        <p>@ViewBag.UploadResult</p>
    </div>
}

@if(Model.Count() == 0) {
    <div>
        <p>No tables have been uploaded.</p>
    </div>
} else {
    <table>
        <tr>
            <th>
                <!-- Something goes here -->
            </th>
        </tr>
        @foreach(var table in Model) {
            <tr>
                <td>
                    <!-- Something goes here -->
                </td>
            </tr>
        }
    </table>
}
